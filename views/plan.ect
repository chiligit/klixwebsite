<!DOCTYPE html>
<html lang="en">

<% include 'head.ect' %>

<body id="page-top" class="index">

    <div id="base_bg"></div>
    <div id="icon_bg"></div>

<!-- Navigation -->
<nav class="navbar navbar-default navbar-fixed-top navbar-shrink">
    <div id="polygon_bg"></div>
    <div class="container">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header page-scroll">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#page-top">
                <img class="img-responsive white" src="img/logo_white_header.png" alt="">
                <img class="img-responsive black" src="img/logo_header.png" alt="">
            </a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
            <ul class="nav navbar-nav navbar-right">
                <li class="hidden">
                    <a href="#page-top"></a>
                </li>               
                <li>
                    <a href="/"><%- @__("navigation.homepage")%></a>
                </li>               
            </ul>
        </div>
        <!-- /.navbar-collapse -->
    </div>
    <!-- /.container-fluid -->
</nav>

<section class="success" id="selectedplan">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center">
                <h2><%- @__("package.selected")%>: <%- @__("package."+ @package + ".name")%></h2>
                <hr class="star-primary medium">
            </div>
        </div>
        <div class="row details">  
            <div class="col-md-8 col-md-offset-2 text-center">   
                <p><%- @__("package."+ @package + ".details.description.pre")%> <%- @__("package."+ @package + ".name")%> <%- @__("package."+ @package + ".details.description")%></p>
            </div>
        </div>
        <div class="row">     
            <div class="col-md-6 col-md-offset-3 text-center">                   
                <ul class="list-table">                        
                    <li><%- @__("package."+ @package + ".details.description2")%></li>
                    <li><%- @__("package."+ @package + ".details.description3")%></li>
                    <li><%- @__("package."+ @package + ".details.description4")%></li>
                    <li><%- @__("package."+ @package + ".details.description5")%></li>
                    <li><%- @__("package."+ @package + ".details.description6")%></li>
				</ul>
            </div>
        </div>
		
		<div class="row">
			<div class="col-md-12 text-center">
				<hr class="star-primary thick">
				<h2><%- @__("form.inquiry")%>:</h2>
				<p id="inquirydescription"><%- @__("form.inquiry.description")%></p>				          
			</div>
		</div>
		<% include 'form.ect' %>

    </div>
</section>

<% include 'footer.ect' %>
<% include 'scripts.ect' %>

</body>

</html>