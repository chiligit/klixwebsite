<section class="success" id="selectedplan">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center">
                <h2><%- @__("package.selected")%>: <%- @__("package."+ @package + ".name")%></h2>
                <hr class="star-primary medium">
            </div>
        </div>
        <div class="row details">  
            <div class="col-md-8 col-md-offset-2">   
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</p>
            </div>
        </div>
        <div class="row">     
            <div class="col-md-6 text-center">                   
                <ul class="list-table">                        
                    <li><%- @__("package."+ @package + ".name")%></li>
                    <li><%- @__("package."+ @package + ".disk")%></li>
                    <li><%- @__("package."+ @package + ".bandwidth")%></li>
                    <li><%- @__("package."+ @package + ".domain")%></li>                    
                </ul>
            </div>
            <div class="col-md-6 text-center">
                <ul class="list-table">                
                    <li>1 dedikált IP</li>
                    <li>100% hálózat uptime</li>
                    <li>24/7 Support</li>
                </ul>
            </div>
        </div>
        <div class="row">     
            <div class="col-md-8 col-md-offset-2 text-center">
                <hr class="star-primary thick">
                <h2>Ajánlatkérés:</h2>    
                <p id="inquirydescription">Kérjük töltse ki az alábbi űrlapot, és felvesszük Önnel a kapcsolatot a részletekről</p>
                <form name="sendInquiry" id="inquiryForm" novalidate>                    
                    <div class="row control-group">
                        <div class="form-group col-xs-12 floating-label-form-group controls">
                            <label>Név*</label>
                            <input type="text" class="form-control" placeholder="Név" id="name" required data-validation-required-message="Please enter your name.">
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="row control-group">
                        <div class="form-group col-xs-12 floating-label-form-group controls">
                            <label>Email*</label>
                            <input type="email" class="form-control" placeholder="Email" id="email" required data-validation-required-message="Please enter your email address.">
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="row control-group">
                        <div class="form-group col-xs-12 floating-label-form-group controls">
                            <label>Telefonszám*</label>
                            <input type="tel" class="form-control" placeholder="Telefonszám" id="phone" required data-validation-required-message="Please enter your phone number.">
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="row control-group">
                        <div class="form-group col-xs-12 floating-label-form-group controls">
                            <label>Megjegyzések</label>
                            <textarea rows="5" class="form-control" placeholder="Üzenet" id="message" required data-validation-required-message="Please enter a message."></textarea>
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="row control-group">
							<div class="form-group captcha-centered controls">
								<div id="captcha" class="g-recaptcha" data-sitekey="6LcQrBATAAAAAGTnr2wsiBC2isCVc-yinSvDHjlN"></div>
								<p id="captchaAlert" class="help-block text-danger" style="display:none">Please complete the captcha.</p>
							</div>
                    </div>
                    <br>
                    <div id="success"></div>
                    <div class="row">
                        <div class="form-group col-xs-12">
                            <button type="submit" class="btn btn-info btn-lg" onclick="checkCaptcha()" >Küldés</button>
                        </div>
                    </div>
                </form>            
            </div>
        </div>
    </div>
</section>

