<section class="success" id="selectedplan">
    <div class="container">
        <div class="row">
            <div class="col-md-12 text-center">
                <h2><%- @__("package.selected")%>: <%- @__("package."+ @package + ".name")%></h2>
                <hr class="star-primary medium">
            </div>
        </div>
        <div class="row details">  
            <div class="col-md-8 col-md-offset-2">   
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat.</p>
            </div>
        </div>
        <div class="row">     
            <div class="col-md-6 text-center">                   
                <ul class="list-table">                        
                    <li><%- @__("package."+ @package + ".name")%></li>
                    <li><%- @__("package."+ @package + ".disk")%></li>
                    <li><%- @__("package."+ @package + ".bandwidth")%></li>
                    <li><%- @__("package."+ @package + ".domain")%></li>                    
                </ul>
            </div>
            <div class="col-md-6 text-center">
                <ul class="list-table">                
                    <li><%- @__("package."+ @package + ".ip")%></li>
                    <li><%- @__("package."+ @package + ".uptime")%></li>
                    <li><%- @__("package."+ @package + ".support")%></li>
                </ul>
            </div>
        </div>
		
		<div class="row">
